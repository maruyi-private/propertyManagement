<view class="content"><view class="input-group"><view class="uni-flex-btw border "><text class="title uni-cell-20">联系人：</text><input class="uni-cell-80" type="text" focus="{{true}}" clearable="{{true}}" placeholder="请输入联系人" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['form']]]]]}}" value="{{form.name}}" bindinput="__e"/></view><view class="uni-flex-btw border"><text class="title uni-cell-20">手机号：</text><input class="uni-cell-80" type="number" clearable="{{true}}" placeholder="请输入联系人手机号" data-event-opts="{{[['input',[['__set_model',['$0','tel','$event',[]],['form']]]]]}}" value="{{form.tel}}" bindinput="__e"/></view></view><button data-event-opts="{{[['tap',[['register',['$event']]]]]}}" class="saveButton" bindtap="__e">保存</button></view>