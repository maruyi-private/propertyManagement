
.bill-detail-head{
	height: 300rpx;
	width: 100%;
	background: #FFFFFF;
}
.bill-detail-content{
	background: #FFFFFF;
	padding: 20rpx;
	margin: 20rpx;
	border-radius: 10rpx;
}
.rule{
	border-top: 1rpx dashed #DDDDDD;
}
.user-list{
	margin-top: 100rpx;
	margin: 20rpx;
}
.user-list-cell{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	    -ms-flex-pack: justify;
	        justify-content: space-between;
	margin: 20rpx;
}
.user-list-cell-right{
	line-height:60rpx;
	text-align: right;
	color: #898989;
	padding-left: 10rpx;
}
.uni-empty{
	text-align: left;
	padding-left: 40rpx;
	font-size: 22rpx;
}

